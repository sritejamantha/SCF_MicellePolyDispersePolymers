#include<stdio.h>
#include<stdlib.h>
#include<math.h>

main()
{
 int i,j,k;
 float r[81]={0.0},PA[81]={0.0},PB[81]={0.0},PS[81]={0.0};
 float SA=0.0,SB=0.0,SS=0.0;
 float const1=0.0,const2=0.0;

 FILE *f1;

 f1 = fopen("DensInp.dat","r");
 
 for(i=0;i<81;i++)
 {
  fscanf(f1,"%f %f %f %f",&r[i],&PA[i],&PB[i],&PS[i]);
 }

 fclose(f1);
 
 const1 = 6.0*1.826/80.0; 
 const2 = 4.0*3.14*pow(const1,2.0);

 for(i=1;i<80;i++)
 {
  SA = SA + PA[i]*pow(i,2.0);
  SB = SB + PB[i]*pow(i,2.0);
  SS = SS + PS[i]*pow(i,2.0); 
 }

 SA = SA + 0.5*(PA[0]*pow(0.0,2.0) + PA[80]*pow(80.0,2.0) ); 
 SB = SB + 0.5*(PB[0]*pow(0.0,2.0) + PB[80]*pow(80.0,2.0) );
 SS = SS + 0.5*(PS[0]*pow(0.0,2.0) + PS[80]*pow(80.0,2.0) ); 

 SA = SA*const2*const1;
 SB = SB*const2*const1;
 SS = SS*const2*const1;

 printf("%f \t %f \t %f \t %f \t %f\n",SA,SB,SS,SA+SB,SA+SB+SS);


 SA=0.0;SB=0.0;SS=0.0;
 for(i=1;i<13;i++)
 {
  SA = SA + PA[i]*pow(i,2.0);
  SB = SB + PB[i]*pow(i,2.0);
  SS = SS + PS[i]*pow(i,2.0); 
 }

 SA = SA + 0.5*(PA[0]*pow(0.0,2.0) + PA[13]*pow(13.0,2.0) ); 
 SB = SB + 0.5*(PB[0]*pow(0.0,2.0) + PB[13]*pow(13.0,2.0) );
 SS = SS + 0.5*(PS[0]*pow(0.0,2.0) + PS[13]*pow(13.0,2.0) ); 

 SA = SA*const2*const1;
 SB = SB*const2*const1;
 SS = SS*const2*const1;

 printf("%f \t %f \t %f \t %f \t %f\n",SA,SB,SS,SA+SB,SA+SB+SS);


} 
